    "standard": "^14.3.4",
    "ts-jest": "^26.0.0",
    "typescript": "^3.9.3"
  },
  "files": [
    "dist/"
  ],
  "standard": {
    "include": [],
    "ignore": [
      "node_modules/",
      "dist/",
      "examples/"
