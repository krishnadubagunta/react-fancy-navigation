{
  "name": "react-fancy-navigation",
  "version": "1.0.0",
  "description": "Fancy Navigation for React",
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf dist && tsc && rollup -c",
    "build-watch": "rollup -c --watch",
    "lint": "standard",
    "format": "prettier-standard --format",
    "test": "jest --coverage"
  },
  "keywords": [
    "navigation",
    "react"
  ],
  "author": "krishnadubagunta",
  "license": "ISC",
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router": "^5.2.0"
  },
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "@types/lodash-es": "^4.17.3",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "jest": "^26.0.1",
    "lodash-es": "^4.17.15",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-jss": "^10.1.1",
    "rollup": "^2.11.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.2",
    "rollup-plugin-typescript2": "^0.27.1",
    "standard": "^14.3.4",
    "standard-prettier": "^1.0.1",
    "ts-jest": "^26.0.0",
    "typescript": "^3.9.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "testPathIgnorePatterns": [
      "build/"
    ]
  },
  "files": [
    "dist/"
  ],
  "standard": {
    "include": [],
    "ignore": [
      "node_modules/",
      "build/",
      "examples/"
    ],
    "globals": [
      "describe",
      "it",
      "test",
      "expect",
      "afterAll",
      "jest"
    ]
  }
}
