    "jest": "^26.0.1",
    "jest-dev-server": "^4.4.0",
    "jest-image-snapshot": "^4.0.2",
    "jest-puppeteer": "~4.4.0",
    "lodash-es": "^4.17.15",
    "puppeteer": "^2.1.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-jss": "^10.1.1",
