  "description": "Fancy Navigation for React",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run start --prefix ./examples",
    "build": "rm -rf dist && tsc && rollup -c",
    "build-watch": "rollup -c --watch",
    "lint": "standard",
    "format": "prettier-standard --format",
    "test": "jest --coverage",
